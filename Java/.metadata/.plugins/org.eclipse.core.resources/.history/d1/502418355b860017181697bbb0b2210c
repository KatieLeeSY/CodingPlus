package Chapter05;

public class ArrayEx17 {

	public static void main(String[] args) {
		if (args.length != 3) {
			System.out.println("usage: java ArrayEx17 NUM1 OP NUM2");
			System.exit(0);
		}
		int num1 = Integer.parseInt(args[0]);	//	문자열을 숫자로 변환한다.
		char op = args[1].charAt(0);			//	문자열을 문자(char)로 변환한다.
		
	}

}
