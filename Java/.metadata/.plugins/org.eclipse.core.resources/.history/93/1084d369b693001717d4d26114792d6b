package Chapter06;

class MyMath2 {
	long a, b;

	// 인스턴스변수 a, b만을 이용해서 작업하므로 매개변수가 필요없다.

	long add() {
		return a + b;
	}

	long subtract() {
		return a - b;
	}

	long multiply() {
		return a * b;
	}

	double divide() {
		return a / b;
	}
}

public class MyMathTest2 {

	public static void main(String[] args) {

	}

}
